<body>
    <div class="element" style="width:100px;height:100px;background:blue;position:relative;"></div>
    <script src="jq_311.js"></script>
    <script src="velocity.js"></script>
	<script>
		$(".element").velocity({
			left: "200px"
		}, {
			duration: 450,
			delay: 1000
